<?xml version="1.0" encoding="utf-8"?>

<configuration>

  <!-- Configuracion de conexion a base de datos -->
  <connectionStrings>
    <add name="ITELConnectionString" connectionString="Data Source=CLAU-NOTEBOOK\MYSQLSERVER;Initial Catalog=ITEL;Integrated Security=True"
      providerName="System.Data.SqlClient" />

    <add name="conString" connectionString="Data Source=CLAU-NOTEBOOK\MYSQLSERVER;Initial Catalog=WebTest;Integrated Security=True"
      providerName="System.Data.SqlClient" />
  </connectionStrings>


  <system.web>
    <compilation debug="true" targetFramework="4.6.1" />
    <httpModules>
      <add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web"/>
    </httpModules>


    <authorization>
      <allow users="?"/>
    </authorization>
  </system.web>





  <appSettings>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="none"/>
  </appSettings>


  <location path="form_nva_noticia.aspx">
    <system.web>
      <authorization>
        <allow users="?"/>
      </authorization>
    </system.web>
  </location>










  <system.webServer>
    <validation validateIntegratedModeConfiguration="false"/>
    <modules>
      <remove name="ApplicationInsightsWebTracking"/>
      <add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web"
        preCondition="managedHandler"/>
    </modules>
  </system.webServer>










</configuration>
